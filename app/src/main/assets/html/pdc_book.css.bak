@charset "UTF-8";
/* CSS Document */
html,body
{
	margin: 0px;
	padding: 0px;  
}
.container-book2f{
	position: relative;
	display: block;
	width: 100%;
	height: auto;
	background-image: url(bg.png);
	background-repeat: repeat;
	padding-top: 20%;
	}
.box-shelf2f{
	position: relative;
	background-image: url(img-shelf.png);
	background-repeat: no-repeat;
	min-height: 700px;
	margin-left: 40px;
	margin-right: 40px;
	padding-left: 87px;
	background-size: 100%;
	}	
.control-book2f{
	position: relative;
	top: -196px;
	}	
.box-book{
	position: relative;
	float: left;
	margin-top: 31px;
	width: 23%;
	padding: 10px 0;
	}	
.box-book img{
	width: 80%;
	-moz-box-shadow: 1px 2px 5px #6b6e73;
	-webkit-box-shadow: 1px 2px 5px #6b6e73;
	box-shadow: 1px 2px 5px #6b6e73;
	}	
			

            
